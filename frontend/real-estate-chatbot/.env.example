# Frontend Environment Variables Configuration
# Copy this file to .env and fill in your actual values
# NEVER commit the .env file to version control

# ================================
# Backend API Configuration
# ================================
# URL for the backend API server
# Default: http://127.0.0.1:5008
VITE_BACKEND_URL=http://127.0.0.1:5008

# ================================
# Application Configuration
# ================================
# Application title (optional)
VITE_APP_TITLE=Real Estate Chatbot

# Application version (optional)
VITE_APP_VERSION=1.0.0

# ================================
# Development Configuration
# ================================
# Enable development features (true/false)
VITE_DEV_MODE=true

# Enable debug logging (true/false)
VITE_DEBUG=false

# ================================
# API Configuration
# ================================
# Default user email for demo purposes
VITE_DEFAULT_USER=demo@email.com

# Request timeout in milliseconds
VITE_REQUEST_TIMEOUT=30000

# ================================
# Example Values
# ================================
# VITE_BACKEND_URL=http://localhost:5008
# VITE_APP_TITLE=Real Estate Chatbot
# VITE_APP_VERSION=1.0.0
# VITE_DEV_MODE=true
# VITE_DEBUG=false