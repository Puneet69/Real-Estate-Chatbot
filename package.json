{
  "name": "real-estate-chatbot",
  "version": "1.0.0",
  "description": "A modern, intelligent real estate chatbot application built with React, Node.js, and AI integration",
  "main": "index.js",
  "scripts": {
    "setup": "./setup.sh",
    "dev": "concurrently \"npm run dev:backend\" \"npm run dev:ai\" \"npm run dev:frontend\"",
    "dev:backend": "cd backend && node server.js",
    "dev:ai": "cd backend/ai_server && python3 main.py",
    "dev:frontend": "cd frontend/real-estate-chatbot && npm run dev",
    "start:backend": "cd backend && node server.js",
    "start:ai": "cd backend/ai_server && python3 main.py",
    "start:frontend": "cd frontend/real-estate-chatbot && npm run dev",
    "build": "cd frontend/real-estate-chatbot && npm run build",
    "install:all": "npm install && cd backend && npm install && cd ../frontend/real-estate-chatbot && npm install",
    "lint": "cd frontend/real-estate-chatbot && npm run lint",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "real-estate",
    "chatbot",
    "react",
    "nodejs",
    "ai",
    "property-search",
    "mongodb",
    "vite"
  ],
  "author": "Puneet69",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Puneet69/Real-Estate-Chatbot.git"
  },
  "bugs": {
    "url": "https://github.com/Puneet69/Real-Estate-Chatbot/issues"
  },
  "homepage": "https://github.com/Puneet69/Real-Estate-Chatbot#readme",
  "dependencies": {
    "cors": "^2.8.5",
    "express": "^5.1.0",
    "mongoose": "^8.19.2",
    "react": "^19.2.0",
    "react-dom": "^19.2.0"
  },
  "devDependencies": {
    "@vitejs/plugin-react": "^5.1.0",
    "vite": "^7.1.12",
    "concurrently": "^8.2.2"
  }
}
